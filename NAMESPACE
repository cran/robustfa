exportPattern("^[[:alpha:]]+")

importFrom(stats4, plot, summary, show)
importFrom(stats, screeplot, biplot, predict)
importFrom(rrcov, getCenter, getEigenvalues, getLoadings, getQuan, getScores, getSdev)

## export functions including S4 generics
export( detail,
	FaClassic,
	FaCov,
	factorScorePca,
	factorScorePfa,
	fsOrder,        
	getFa,                        
	myFaPrint,      
	myplotDD,        
	plot,     
	predict,      
	computeScores,   
	print,     
        show,          
	summary
)

## S3 generics
S3method(FaClassic, default)
S3method(FaClassic, formula)

S3method(FaCov, default)
S3method(FaCov, formula)

exportMethods(
    getCenter,
    getEigenvalues,
    getFa,
    getLoadings,
    getQuan,
    getScores,
    getSdev,
    plot,
    predict,
    print,
    show,
    summary 
)

exportClasses(
     Fa,
